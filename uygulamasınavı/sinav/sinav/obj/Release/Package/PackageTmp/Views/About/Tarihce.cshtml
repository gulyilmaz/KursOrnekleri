@model sinav.Models.ProductsModelView
@{
    ViewBag.Title = "Tarihce";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="tm-container-inner tm-history">
    <div class="row">
        <div class="col-12">
            <div class="tm-history-inner">
                <img src="~/Content/img/about-06.jpg" alt="Image" class="img-fluid tm-history-img" />
                <div class="tm-history-text">
                    <h4 class="tm-history-title">@Model.tarihce.başlik</h4>
                    <p class="tm-mb-p">
                        @Model.tarihce.aciklama
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="tm-container-inner tm-featured-image">
    <div class="row">
        <div class="col-12">
            <div class="placeholder-2">
                <div class="parallax-window-2" data-parallax="scroll" data-image-src="/Content/img/about-05.jpg"></div>
            </div>
        </div>
    </div>
</div>
<div class="tm-container-inner tm-features">
    <div class="row">

        @foreach (var item in Model.Olist)
        {<div class="col-lg-4">
                <div class="tm-feature">
                    <i class="fas fa-4x @item.icon tm-feature-icon"></i>
                    <p class="tm-feature-description">@item.açiklama</p>
                    <a href="/Home/Index" class="tm-btn tm-btn-@item.butonrenk">Daha Fazlası</a>
                </div>
            </div>
        }



    </div>
</div>

