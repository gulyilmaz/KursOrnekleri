@model Route_Yonlendirme.Models.usertablo
@{
    ViewBag.Title = "Profile";
}

<h2>Profile</h2>
<table class="table table-striped table-bordered">
    <tr>
        <td>User ID</td>
        <td>@Model.id</td>
    </tr>
    <tr>
        <td>First Name</td>
        <td>@Model.firstname</td>
    </tr>
    <tr>
        <td>Last Name</td>
        <td>@Model.lastname</td>
    </tr>
</table>

